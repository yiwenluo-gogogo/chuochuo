{
  "manifest_version": 3,
  "name": "It's Your Turn Today Extension",
  "version": "0.1",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
      // "js": ["jquery-3.6.0.js", "socket.io.min.js", "content.js"]
    }
  ],
  "action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "contextMenus"
   ]
}