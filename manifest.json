{
  "manifest_version": 2,
  "name": "It's Your Turn Today Extension",
  "version": "0.1",
  "background": {
    "scripts": [
      // "socket.io.min.js",
      "background.js"],
      "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
      // "js": ["jquery-3.6.0.js", "socket.io.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "contextMenus"
   ]
}